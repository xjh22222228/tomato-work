    "modulePaths": [
      "/Users/xiejiahe/Documents/nginx/development/client/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
